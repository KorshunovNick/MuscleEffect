{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "build",
    "typeRoots": ["./server/types","node_modules/@types"]
  },
  "include": ["server/**/*"],
  "exclude": ["front","build"]
}
